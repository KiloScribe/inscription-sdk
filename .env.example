# API Configuration
API_KEY=your_api_key

# Hedera Configuration
HEDERA_ACCOUNT_ID=your_account_id
HEDERA_PRIVATE_KEY=your_private_key
# Key type for raw hex private keys (ed25519 or ecdsa)
# Only needed if HEDERA_PRIVATE_KEY is in raw hex format (e.g., 0x...)
# DER and PEM formats are auto-detected
HEDERA_KEY_TYPE=ed25519
HEDERA_NETWORK=testnet # or mainnet

# Inscription Configuration
HOLDER_ID=your_holder_id
